Cell[TextData[{
 "\nThe following is easy to verify:\n\n\t",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["B", "i", "n"], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"n", "+", "1", "-", "i"}], 
    RowBox[{"n", "+", "1"}]], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["B", "i", 
    RowBox[{"n", "+", "1"}]], TraditionalForm]]],
 " + ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"i", "+", "1"}], 
    RowBox[{"n", "+", "1"}]], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["B", 
    RowBox[{"i", "+", "1"}], 
    RowBox[{"n", "+", "1"}]], TraditionalForm]]],
 ".\n\t\nThus\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"i", "=", "0"}], "n"], 
    SubsuperscriptBox["B", "i", "n"]}], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    StyleBox["b",
     FontWeight->"Bold"], "i"], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderoverscriptBox["\[Sum]", 
     RowBox[{"i", "=", "0"}], "n"], 
    RowBox[{"[", 
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"n", "+", "1", "-", "i"}], 
        RowBox[{"n", "+", "1"}]], 
       SubsuperscriptBox["B", "i", 
        RowBox[{"n", "+", "1"}]]}], "+", 
      RowBox[{
       FractionBox[
        RowBox[{"i", "+", "1"}], 
        RowBox[{"n", "+", "1"}]], 
       SubsuperscriptBox["B", 
        RowBox[{"i", "+", "1"}], 
        RowBox[{"n", "+", "1"}]]}]}], " ", "]"}]}], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    StyleBox["b",
     FontWeight->"Bold"], "i"], TraditionalForm]]],
 "\n\t\n\t\t     =",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "0"}], "n"], 
     RowBox[{
      FractionBox[
       RowBox[{"n", "+", "1", "-", "i"}], 
       RowBox[{"n", "+", "1"}]], 
      SubsuperscriptBox["B", "i", 
       RowBox[{"n", "+", "1"}]], 
      SubscriptBox[
       StyleBox["b",
        FontWeight->"Bold"], "i"]}]}], "+", " ", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "0"}], "n"], 
     RowBox[{
      FractionBox[
       RowBox[{"i", "+", "1"}], 
       RowBox[{"n", "+", "1"}]], 
      SubsuperscriptBox["B", 
       RowBox[{"i", "+", "1"}], 
       RowBox[{"n", "+", "1"}]], " "}]}]}], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    StyleBox["b",
     FontWeight->"Bold"], "i"], TraditionalForm]]],
 "\n\t\t     \n\t\t     = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "0"}], 
      StyleBox[
       RowBox[{"n", "+", "1"}],
