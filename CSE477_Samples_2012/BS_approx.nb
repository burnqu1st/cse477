Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"n", "=", "1"}], ";"}], "  ", 
  RowBox[{"(*", " ", "degree", " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"m", "=", "70"}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"counting", " ", "from", " ", "0", " ", "to", " ", "m"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ll", "=", "1"}], ";"}], "  ", 
  RowBox[{"(*", " ", 
   RowBox[{"domain", " ", "intervals"}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"tt1", "=", 
    RowBox[{"Table", "[", 
     RowBox[{"t", ",", 
      RowBox[{"{", 
       RowBox[{"t", ",", "0.0", ",", "1.0", ",", 
        RowBox[{"1", "/", "m"}]}], "}"}]}], "]"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"data", " ", "point", " ", "parameter", " ", "values"}], " ", 
   "*)"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"knot", "=", 
    RowBox[{
     RowBox[{"Join", "[", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{"0", ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "0", ",", "n"}], "}"}]}], "]"}], ",", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"i", "/", "ll"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", 
           RowBox[{"ll", "-", "1"}]}], "}"}]}], "]"}], ",", 
       RowBox[{"Table", "[", 
        RowBox[{"1", ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "0", ",", "n"}], "}"}]}], "]"}]}], "]"}], "//", 
     "N"}]}], ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"data1", "=", 
    RowBox[{
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"2", "+", "t"}], ")"}], 
           RowBox[{"Cos", "[", "t", "]"}]}], "+", " ", 
          RowBox[{"2", 
           RowBox[{"RandomReal", "[", "]"}]}]}], ",", " ", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"2", "+", "t"}], ")"}], 
           RowBox[{"Sin", "[", "t", "]"}]}], "+", " ", 
          RowBox[{"2", 
           RowBox[{"RandomReal", "[", "]"}]}]}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"t", ",", "0", ",", "10.0", ",", 
         RowBox[{"10", "/", "m"}]}], "}"}]}], "]"}], "//", "N"}]}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "use", " ", "a", " ", "different", " ", "order", " ", "to", " ", 
     "display", " ", "our", " ", "geometry"}], ",", " ", 
    RowBox[{"simply", " ", "for", " ", "better", " ", "viewing"}]}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"approx1", "[", 
    RowBox[{"n_", ",", "knot_", ",", "data_", ",", "tt_"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"m", "=", 
      RowBox[{
       RowBox[{"Length", "[", "data", "]"}], "-", "1"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ll", "=", 
      RowBox[{
       RowBox[{"Length", "[", "knot", "]"}], "-", 
       RowBox[{"2", "*", "n"}], "-", "1"}]}], ";", " ", 
     RowBox[{"(*", " ", 
      RowBox[{"domain", " ", "intervals"}], " ", "*)"}], 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "van", " ", "is", " ", "the", " ", "Vandermonde", " ", "matrix", " ", 
       "which", " ", "typically", " ", "has", " ", 
       RowBox[{"(", "many", ")"}], " ", "more", " ", "roes", " ", "than", " ",
        "columns"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"van", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"BSplineBasis", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"n", ",", "knot"}], "}"}], ",", "j", ",", 
          RowBox[{"tt", "[", 
           RowBox[{"[", "i", "]"}], "]"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"m", "+", "1"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "0", ",", 
          RowBox[{"n", "+", "ll", "-", "1"}]}], " ", "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"dd", "=", 
      RowBox[{"LinearSolve", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Transpose", "[", "van", "]"}], ".", "van"}], ",", 
        RowBox[{
         RowBox[{"Transpose", "[", "van", "]"}], ".", "data"}]}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"dataplot", "=", 
      RowBox[{"ListPlot", "[", 
       RowBox[{"data", ",", 
        RowBox[{"PlotStyle", "\[Rule]", " ", "Thick"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"points", "=", 
      RowBox[{"Graphics", "[", 
       RowBox[{"{", 
        RowBox[{"Red", ",", 
         RowBox[{"PointSize", "[", "0.015", "]"}], ",", 
         RowBox[{"Point", "[", "dd", "]"}]}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ddplot", "=", 
      RowBox[{"ListLinePlot", "[", "dd", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"curve", "[", "t_", "]"}], ":=", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"BSplineBasis", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"n", ",", "knot"}], "}"}], ",", "i", ",", "t"}], "]"}], 
         "*", 
         RowBox[{"dd", "[", 
          RowBox[{"[", 
           RowBox[{"i", "+", "1"}], "]"}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "0", ",", 
          RowBox[{"n", "+", "ll", "-", "1"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"cplot", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{
        RowBox[{"curve", "[", "t", "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", 
          RowBox[{"knot", "[", 
           RowBox[{"[", "1", "]"}], "]"}], ",", 
          RowBox[{"knot", "[", 
           RowBox[{"[", 
            RowBox[{
             RowBox[{"2", "n"}], "+", "ll", "+", "1"}], "]"}], "]"}]}], "}"}],
         ",", 
        RowBox[{"PlotStyle", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"Thick", ",", "Green"}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"approxpic", "=", 
      RowBox[{"Show", "[", 
       RowBox[{
        RowBox[{"{", 
